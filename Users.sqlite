--EXPLORING THE DATABASDE TABLE USERS
 SELECT * FROM users
 LIMIT 20;

--QUERY FOR FINDING EMAIL ADDRESSES AND BIRTHDAY OF USERS IN 80'S DECADE.
 SELECT email, birthday
 FROM users
 WHERE birthday BETWEEN '1980-01-01' and '1989-12-31';

 --QUERY FOR FINDING EMAILS AND CREATION_DATE OF USERS SIGNED BEFORE AN DATE
 SELECT emails, created_at
 FROM users
 WHERE created _at  < '2017-05-01';

--QUERY FOR FINDING EMAILS OF GROUP OF USERS WHO WERE PART OF A/B TEST AND RECEIVED "BEARS" TEST.
 SELECT email
 FROM users
 WHERE test = 'bears';

--QUERY FOR FINDIN EMAILS OF ALL USERS WHO RECEIVED A CAMPIAGN ON WEBSITE BBB.
SELECT email
FROM users
WHERE campaign LIKE 'BBB%';

--QUERY FRO FINDING EMAILS OF ALL USERS WHO RECEIVED AD-COPY 2 IN THEIR CAMPAIGN.
SELECT email
FROM users
WHERE campaign LIKE '%-2';

--QUERY FRO FINDING EMAILS OF ALL USERS WHO RECEIVED BOTH CAMPIAGN AND A TEST.
SELECT email
FROM users
WHERE campaign IS NOT NULL
  AND test IS NOT NULL;
